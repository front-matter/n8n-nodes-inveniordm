{
	"name": "n8n-nodes-inveniordm",
	"version": "0.1.2",
	"description": "n8n node for InvenioRDM API",
	"license": "MIT",
	"homepage": "https://github.com/front-matter/n8n-nodes-inveniordm",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "Martin Fenner",
		"email": "martin@front-matter.io"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/front-matter/n8n-nodes-inveniordm.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:e2e": "jest --testPathPattern=e2e",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": false,
		"credentials": [
			"dist/credentials/InvenioRDMApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/inveniordm/Inveniordm.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/jest": "^29.5.12",
		"eslint": "9.32.0",
		"jest": "^29.7.0",
		"nock": "^13.4.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"ts-jest": "^29.1.2",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
